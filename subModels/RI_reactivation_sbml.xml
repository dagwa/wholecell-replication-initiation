<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<sbml xmlns="http://www.sbml.org/sbml/level3/version1/core" level="3" version="1">
  <model id="RepInit_Reactivation" name="RepInit_Reactivation">
    <listOfFunctionDefinitions>
      <functionDefinition id="minFunc">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <lambda>
            <bvar>
              <ci>x</ci>
            </bvar>
            <bvar>
              <ci>y</ci>
            </bvar>
            <piecewise>
              <piece>
                <ci>x</ci>
                <apply>
                  <lt/>
                  <ci>x</ci>
                  <ci>y</ci>
                </apply>
              </piece>
              <otherwise>
                <ci>y</ci>
              </otherwise>
            </piecewise>
          </lambda>
        </math>
      </functionDefinition>
      <functionDefinition id="stochasticRound">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <lambda>
            <bvar>
              <ci>x</ci>
            </bvar>
            <apply>
              <plus/>
              <apply>
                <floor/>
                <ci>x</ci>
              </apply>
              <apply>
                <floor/>
                <apply>
                  <times/>
                  <cn>2</cn>
                  <apply>
                    <ci>random</ci>
                  </apply>
                </apply>
              </apply>
            </apply>
          </lambda>
        </math>
      </functionDefinition>
    </listOfFunctionDefinitions>
    <listOfParameters>
      <parameter constant="false" id="ADP" value="0.0"/>
      <parameter constant="false" id="ATP" value="0.0"/>
      <parameter constant="false" id="DnaA_1mer_ADP" value="0.0"/>
      <parameter constant="false" id="DnaA_1mer_ATP" value="0.0"/>
      <parameter constant="false" id="K_Regen_P4" value="0.018"/>
      <parameter constant="false" id="cell_Volume" value="0.0"/>
      <parameter constant="false" id="k_Regen" value="2.3026"/>
      <parameter constant="false" id="m_membrane" value="0.0"/>
      <parameter constant="false" id="membraneConc" value="0.0"/>
      <parameter constant="false" id="numRegenerations" value="0.0"/>
      <parameter constant="false" id="reactivation" value="0.0"/>
      <parameter constant="false" id="unknown" value="0.0"/>
      <parameter constant="false" id="x" value="0.0"/>
      <parameter constant="false" id="y" value="0.0"/>
    </listOfParameters>
    <listOfInitialAssignments>
      <initialAssignment metaid="numRegenerations_Initialization" symbol="numRegenerations">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <ci>minFunc</ci>
            <ci>ATP</ci>
            <apply>
              <ci>minFunc</ci>
              <ci>DnaA_1mer_ADP</ci>
              <apply>
                <ci>stochasticRound</ci>
                <apply>
                  <divide/>
                  <apply>
                    <times/>
                    <ci>DnaA_1mer_ADP</ci>
                    <apply>
                      <times/>
                      <apply>
                        <divide/>
                        <ci>k_Regen</ci>
                        <cn>3600</cn>
                      </apply>
                      <ci>membraneConc</ci>
                    </apply>
                  </apply>
                  <apply>
                    <plus/>
                    <ci>K_Regen_P4</ci>
                    <ci>membraneConc</ci>
                  </apply>
                </apply>
              </apply>
            </apply>
          </apply>
        </math>
      </initialAssignment>
    </listOfInitialAssignments>
    <listOfRules>
      <assignmentRule metaid="membraneConcInit" variable="membraneConc">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <divide/>
            <ci>m_membrane</ci>
            <ci>cell_Volume</ci>
          </apply>
        </math>
      </assignmentRule>
    </listOfRules>
    <listOfEvents>
      <event id="endReactivation" useValuesFromTriggerTime="true">
        <trigger initialValue="false" persistent="true">
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <and/>
              <apply>
                <leq/>
                <ci>numRegenerations</ci>
                <cn>0</cn>
              </apply>
              <apply>
                <eq/>
                <ci>reactivation</ci>
                <cn>1</cn>
              </apply>
            </apply>
          </math>
        </trigger>
        <listOfEventAssignments>
          <eventAssignment variable="reactivation">
            <math xmlns="http://www.w3.org/1998/Math/MathML">
              <cn>0</cn>
            </math>
          </eventAssignment>
        </listOfEventAssignments>
      </event>
      <event id="reactivationEvent" useValuesFromTriggerTime="true">
        <trigger initialValue="false" persistent="true">
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <and/>
              <apply>
                <gt/>
                <ci>numRegenerations</ci>
                <cn>0</cn>
              </apply>
              <apply>
                <eq/>
                <ci>reactivation</ci>
                <cn>1</cn>
              </apply>
            </apply>
          </math>
        </trigger>
        <listOfEventAssignments>
          <eventAssignment variable="DnaA_1mer_ADP">
            <math xmlns="http://www.w3.org/1998/Math/MathML">
              <apply>
                <minus/>
                <ci>DnaA_1mer_ADP</ci>
                <ci>numRegenerations</ci>
              </apply>
            </math>
          </eventAssignment>
          <eventAssignment variable="DnaA_1mer_ATP">
            <math xmlns="http://www.w3.org/1998/Math/MathML">
              <apply>
                <plus/>
                <ci>DnaA_1mer_ATP</ci>
                <ci>numRegenerations</ci>
              </apply>
            </math>
          </eventAssignment>
          <eventAssignment variable="ATP">
            <math xmlns="http://www.w3.org/1998/Math/MathML">
              <apply>
                <minus/>
                <ci>ATP</ci>
                <ci>numRegenerations</ci>
              </apply>
            </math>
          </eventAssignment>
          <eventAssignment variable="ADP">
            <math xmlns="http://www.w3.org/1998/Math/MathML">
              <apply>
                <plus/>
                <ci>ADP</ci>
                <ci>numRegenerations</ci>
              </apply>
            </math>
          </eventAssignment>
        </listOfEventAssignments>
      </event>
    </listOfEvents>
  </model>
</sbml>
