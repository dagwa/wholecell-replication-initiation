                <ci>numRegenerations</ci>
                <cn>0.0</cn>
              </apply>
              <apply>
                <eq/>
                <ci>reactivation</ci>
                <cn>1.0</cn>
              </apply>
            </apply>
          </math>
        </trigger>
        <listOfEventAssignments>
          <eventAssignment variable="DnaA_1mer_ADP__c">
            <math xmlns="http://www.w3.org/1998/Math/MathML">
              <apply>
                <minus/>
                <ci>DnaA_1mer_ADP__c</ci>
