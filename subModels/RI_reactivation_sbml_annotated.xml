  <model id="RepInit_Reactivation" name="RepInit_Reactivation">
    <listOfFunctionDefinitions>
      <functionDefinition id="fractionalPart">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <lambda>
            <bvar>
              <ci>x</ci>
            </bvar>
            <apply>
              <minus/>
              <ci>x</ci>
              <apply>
                <floor/>
                <ci>x</ci>
              </apply>
            </apply>
          </lambda>
        </math>
      </functionDefinition>
      <functionDefinition id="minFunc">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <lambda>
            <bvar>
              <ci>x</ci>
            </bvar>
            <bvar>
              <ci>y</ci>
            </bvar>
            <piecewise>
              <piece>
                <ci>x</ci>
                <apply>
                  <lt/>
                  <ci>x</ci>
                  <ci>y</ci>
                </apply>
              </piece>
              <otherwise>
                <ci>y</ci>
              </otherwise>
            </piecewise>
          </lambda>
        </math>
      </functionDefinition>
      <functionDefinition id="stochasticRound">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <lambda>
            <bvar>
              <ci>x</ci>
            </bvar>
            <piecewise>
              <piece>
                <apply>
                  <plus/>
                  <apply>
                    <floor/>
                    <ci>x</ci>
                  </apply>
                  <cn>1.0</cn>
                </apply>
                <apply>
                  <lt/>
                  <apply>
                    <ci>random</ci>
                  </apply>
                  <apply>
                    <ci>fractionalPart</ci>
                    <ci>x</ci>
                  </apply>
                </apply>
              </piece>
              <otherwise>
                <apply>
                  <floor/>
                  <ci>x</ci>
                </apply>
              </otherwise>
            </piecewise>
          </lambda>
        </math>
      </functionDefinition>
    </listOfFunctionDefinitions>
    <listOfCompartments>
