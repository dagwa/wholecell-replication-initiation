            </math>
          </eventAssignment>
          <eventAssignment variable="ADP__c">
            <math xmlns="http://www.w3.org/1998/Math/MathML">
              <apply>
                <plus/>
                <ci>ADP__c</ci>
                <ci>numRegenerations</ci>
              </apply>
            </math>
          </eventAssignment>
        </listOfEventAssignments>
      </event>
